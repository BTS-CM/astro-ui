{
  "name": "bitshares_astro_ui",
  "productName": "Bitshares Astro UI",
  "version": "0.2.12",
  "main": "app/background.js",
  "homepage": "https://github.com/bts-cm/astro-ui",
  "repository": "github:bts-cm/astro-ui",
  "author": "BeetEOS team <nftea.gallery@gmail.com>",
  "description": "Bitshares market UI",
  "license": "MIT",
  "build": {
    "appId": "bitshares_astro_ui",
    "productName": "Bitshares Astro UI",
    "artifactName": "BTSAstroUI.${ext}",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "./astroDist",
        "to": "astroDist"
      }
    ],
    "directories": {
      "buildResources": "resources",
      "output": "dist"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "category": "Utility",
      "target": [
        "deb",
        "AppImage"
      ],
      "mimeTypes": [],
      "executableName": "BTSAstroUI %U"
    }
  },
  "dependencies": {
    "@astrojs/node": "^9.2.2",
    "@astrojs/react": "^4.3.0",
    "@astrojs/tailwind": "^6.0.2",
    "@babel/runtime": "^7.27.1",
    "@exodus/bytebuffer": "git@github.com:BTS-CM/bytebuffer.js.git#393541bd29f988a2af6a7785b1ee4978881ed6bb",
    "@hookform/resolvers": "^3.10.0",
    "@nanostores/persistent": "^1.0.0",
    "@nanostores/query": "^0.3.4",
    "@nanostores/react": "^1.0.0",
    "@noble/hashes": "^1.8.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-hook/throttle": "^2.2.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "assert": "^2.1.0",
    "astro": "^5.13.2",
    "bigi": "^1.4.2",
    "bignumber.js": "^9.3.0",
    "bitsharesjs": "https://github.com/bitshares/bitsharesjs#develop",
    "bitsharesjs-ws": "^6.0.2",
    "bs58": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "create-hash": "^1.2.0",
    "create-hmac": "^1.1.7",
    "date-fns": "^3.6.0",
    "deep-equal": "^2.2.3",
    "dompurify": "^3.2.6",
    "ecurve": "^1.0.6",
    "embla-carousel-react": "^8.6.0",
    "fuse.js": "^7.1.0",
    "input-otp": "^1.4.2",
    "isomorphic-ws": "^5.0.0",
    "long": "^5.3.2",
    "lucide-react": "^0.427.0",
    "mime-types": "^3.0.1",
    "nanostores": "^1.0.1",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.4",
    "react-i18next": "^15.4.1",
    "react-intersection-observer": "^9.15.1",
    "react-resizable-panels": "^2.1.9",
    "react-window": "^1.8.11",
    "recharts": "^2.15.3",
    "safe-buffer": "^5.2.1",
    "shadcn": "^2.5.0",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.3",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "babel-loader": "^9.2.1",
    "electron": "^36.3.1",
    "electron-builder": "^26.0.12",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "eraseChainData": "rm -rf ./src/data/bitshares/* && rm -rf ./src/data/bitshares_testnet/*",
    "erasePools": "rm -rf ./src/data/bitshares/allPools.json && rm -rf ./src/data/bitshares/minPools.json && rm -rf ./src/data/bitshares/pools.json && rm -rf ./src/data/bitshares_testnet/allPools.json && rm -rf ./src/data/bitshares_testnet/minPools.json && rm -rf ./src/data/bitshares_testnet/pools.json",
    "eraseAssets": "rm -rf ./src/data/bitshares/allAssets.json && rm -rf ./src/data/bitshares/minAssets.json && rm -rf ./src/data/bitshares_testnet/allAssets.json && rm -rf ./src/data/bitshares_testnet/minAssets.json",
    "eraseDynamicData": "rm -rf ./src/data/bitshares/dynamicData.json && rm -rf ./src/data/bitshares_testnet/dynamicData.json",
    "eraseIssuers": "rm -rf ./src/data/bitshares/assetIssuers.json && rm -rf ./src/data/bitshares_testnet/assetIssuers.json",
    "eraseBitassetData": "rm -rf ./src/data/bitshares/bitassetData.json && rm -rf ./src/data/bitshares/minBitassetData.json && rm -rf ./src/data/bitshares_testnet/bitassetData.json && rm -rf ./src/data/bitshares_testnet/minBitassetData.json",
    "eraseOffers": "rm -rf ./src/data/bitshares/allOffers.json && rm -rf ./src/data/bitshares_testnet/allOffers.json",
    "eraseFees": "rm -rf ./src/data/bitshares/fees.json && rm -rf ./src/data/bitshares_testnet/fees.json",
    "a": "",
    "refreshData": "rm -rf ./src/data/bitshares/* && rm -rf ./src/data/bitshares_testnet/* && bun run initData",
    "initData": "bun run fetchPools && bun run fetchFees && bun run fetchAssets && bun run fetchDynamicData && bun run fetchIssuers && bun run fetchBitassetData && bun run fetchOffers",
    "fetchPools": "bun run ./src/data/fetchPools.js",
    "fetchAssets": "bun run ./src/data/fetchAssets.js",
    "fetchDynamicData": "bun run ./src/data/fetchDynamicData.js",
    "fetchIssuers": "bun run ./src/data/fetchAssetIssuers.js",
    "fetchBitassetData": "bun run ./src/data/fetchBitassetData.js",
    "fetchOffers": "bun run ./src/data/fetchCreditOffers.js",
    "fetchDeals": "bun run ./src/data/fetchCreditDeals.js",
    "fetchFees": "bun run ./src/data/fetchFees.js",
    "b": "",
    "dev": "astro dev",
    "build:astro": "astro build",
    "start": "node build/start.js",
    "restart": "npm run-script build:astro && npm run-script start",
    "compile": "webpack --config=build/webpack.main.config.js --config=build/webpack.preload.config.js --env=production",
    "dist:macos-latest": "npm run-script compile && electron-builder -m --publish never",
    "dist:windows-latest": "npm run-script compile && electron-builder -w --publish never",
    "dist:ubuntu-latest": "npm run-script compile && electron-builder -l --publish never"
  }
}
